// dadi Specific Parameters for Likelihood Ratio Comparisons between already generated & optimized models
// See dadi_params_README.md or json_structure_overview.md for additional clarification on parameter inputs

{
    "DATA_SFS":"path/to/data/sfs",

    // Model and its optimized parameters that you will be testing
    "TEST MODEL": ["dadi.Demographics2D.split_mig", ["list of optimized model parameters"]],

    // Model and its optimized parameters that you will be testing
    "NULL MODEL": ["dadi.Demographics2D.snm", ["list of optimized model parameters"]],

    // This is a list that indicates which positions in the complex model arguments are fixed to create the simple model for a likelihood ratio test.
    // See dadi documentation for in-depth description on how to use LRT
    "NESTED INDICES": [3],

    "RESULT OUTDIR": "path/to/store/results",

    "BOOTSTRAP DIR"
}
